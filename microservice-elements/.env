# Infrastructure Environment Variables

# Portainer
PORTAINER_ADMIN_USERNAME=admin
PORTAINER_ADMIN_PASSWORD_HASH=

# Redis
REDIS_PASSWORD=your-redis-password-here

# RabbitMQ
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=your-rabbitmq-password-here
RABBITMQ_VHOST=/
RABBITMQ_ERLANG_COOKIE=secure-cookie-change-me

# MongoDB
MONGODB_USERNAME=admin
MONGODB_PASSWORD=your-mongodb-password-here
MONGODB_DATABASE=application_db

# InfluxDB
INFLUXDB_USERNAME=admin
INFLUXDB_PASSWORD=your-influxdb-password
INFLUXDB_ORG=my-org
INFLUXDB_BUCKET=my-bucket
INFLUXDB_TOKEN=my-super-secret-auth-token
INFLUXDB_RETENTION=2d

